"apache_elk_example" : {
    "order" : 0,
    "index_patterns" : [
      "apache_elk_example"
    ],
    "settings" : {
      "index" : {
        "refresh_interval" : "5s"
      }
    },
    "mappings" : {
      "_default_" : {
        "dynamic_templates" : [
          {
             "notanalyzed": {
               "match_mapping_type": "string",
               "mapping": {
                   "type": "keyword"
                },
                "match": "*"
              }
          },
          {
            "message_field" : {
              "mapping" : {
                "omit_norms" : true,
                "type" : "text"
              },
              "match_mapping_type" : "text"
            }
          },
          {
            "string_fields" : {
              "mapping" : {
                "type" : "text"
              },
              "match_mapping_type" : "text"
            }
          }
        ],
        "properties" : {
          "geoip" : {
            "dynamic" : true,
            "properties" : {
              "location" : {
                "type" : "geo_point"
              }
            },
            "type" : "object"
          }
        }
      }
    },
    "aliases" : { }
  }
